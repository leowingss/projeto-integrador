<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylesheets/equipamentos.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css" />
    <link rel="stylesheet" type="text/css" href="/stylesheets/slick.css" />
    <title>
        Equipamentos
    </title>
</head>

<body>
    <!-- Header da página Home -->
    <%- include('partials/header') %>
        <!-- Header da página Home -->


        <!-- Slide de imagens -->

        <%- include('partials/slide') %>

            <!-- Slide de imagens -->



            <!-- Seção de produtos -->

            <section class="produtos">

                <%produtos.forEach(produto =>{ %>
                    <div class="item">
                        <img src=<%=produto.image%> alt=
                        <%= produto.nome %> >
                            <p class="nome-produto">
                                <%=produto.nome%>
                            </p>
                            <span>R$<%=produto.preco%></span>
                            <a class="btn">Comprar</a>
                    </div>
                    <% } ) %>

            </section>


            <!-- Seção de produtos -->


            <!-- Footer -->

            <%- include('partials/footer') %>

                <!-- Footer -->



                <script type="text/javascript" src="/stylesheets/jquery.js"></script>
                <script type="text/javascript" src="/stylesheets/jquery-migrate.js"></script>
                <script type="text/javascript" src="/stylesheets/slick.min.js"></script>
                <script type="text/javascript" src="/main.js"></script>

</body>

</html>